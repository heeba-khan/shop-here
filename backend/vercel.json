
  {
    "version": 2,
    "builds": [
      {
        "src": "backend/api/app.js",    
        "use": "@vercel/node"            
      }
    ],
    "routes": [
      {
        "src": "/api/auth(.*)",              
        "dest": "/backend/api/app.js"    
      },
      {
        "src": "/",                     
        "dest": "/backend/api/app.js"   
      }
    ]
  }
  